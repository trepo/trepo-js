{
  "name": "trepo-core",
  "version": "0.2.1",
  "description": "Core graphql implementation of Trepo",
  "keywords": [
    "Trepo",
    "graphql",
    "genealogy",
    "graph",
    "database"
  ],
  "main": "dist/Trepo.js",
  "scripts": {
    "build": "../../node_modules/.bin/babel src --out-dir dist --ignore *.test.js",
    "test": "../../node_modules/.bin/mocha --compilers js:babel-register --register babel-register 'src/**/*.test.js' -R list",
    "watch": "../../node_modules/.bin/babel src --out-dir dist --ignore *.test.js -w"
  },
  "author": "John Clark <socrates37@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/trepo/trepo-js"
  },
  "bugs": {
    "url": "https://github.com/trepo/trepo-js/issues"
  },
  "license": "MIT",
  "dependencies": {
    "graphql": "0.9.1",
    "graphql-tools": "0.10.1",
    "trepo-ptree": "^0.2.1",
    "trepo-vgraph": "^0.2.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
